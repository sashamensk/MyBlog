@model IEnumerable<MyBlog.Areas.Admin.Models.User>
 
<h2>UserList</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">User Rights</a>
            </td>
        </tr>
    }
</table>